SELECT 
    roomtable.RoomNumber,
    reservationtable.Adults + reservationtable.Children Guests,
    reservationtable.StartDate,
    reservationtable.EndDate
FROM
    reservationtable
        INNER JOIN
    roomtable ON reservationtable.RoomNumber = roomtable.RoomNumber
WHERE
    reservationtable.Adults + reservationtable.Children >= 3
        AND (reservationtable.StartDate BETWEEN '2023-4-1' AND '2023-4-30'
        OR reservationtable.EndDate BETWEEN '2023-4-1' AND '2023-4-30');
/*
Returns Nothing
*/

SELECT 
    guesttable.FirstName,
    guesttable.LastName,
    COUNT(*) 'Number of Reservations'
FROM
    reservationtable
        INNER JOIN
    guesttable ON reservationtable.GuestID = guesttable.GuestID
GROUP BY guesttable.GuestID
ORDER BY COUNT(*) DESC , guesttable.LastName;
/*
Mack		Simmer		4
Bettyann	Seery		3
Duane		Cullison	2
Walter		Holaway		2
Aurore		Lipton		2
Tim			Pattison	2
Maritza		Tilton		2
Joleen		Tison		2
Wilfred		Vise		2
Karie		Yang		2
Zachery		Luechtefeld	1
*/

SELECT 
    guesttable.FirstName,
    guesttable.LastName,
    guesttable.Address,
    guesttable.Phone
FROM
    guesttable
WHERE
    guesttable.Phone = '3084940198';

/*
Duane	Cullison	9662 Foxrun Lane	3084940198
*/

