-- QUERY 1
SELECT 
    guesttable.FirstName,
    guesttable.LastName,
    reservationtable.RoomNumber,
    reservationtable.StartDate,
    reservationtable.EndDate
FROM
    reservationtable
        INNER JOIN
    guesttable ON reservationtable.GuestID = guesttable.GuestID
WHERE
    reservationtable.EndDate BETWEEN '2023-7-1' AND '2023-7-31';
/*
Tim			Pattison	205		2023-06-28	2023-07-02
Walter		Holaway		204		2023-07-13	2023-07-14
Wilfred		Vise		401		2023-07-18	2023-07-21
Bettyann	Seery		303		2023-07-28	2023-07-29
*/

SELECT 
    guesttable.FirstName,
    guesttable.LastName,
    reservationtable.RoomNumber,
    reservationtable.StartDate,
    reservationtable.EndDate
FROM
    reservationtable
        INNER JOIN
    guesttable ON reservationtable.GuestID = guesttable.GuestID
        INNER JOIN
    roomamenitiestable ON reservationtable.RoomNumber = roomamenitiestable.RoomNumber
        INNER JOIN
    amenitiestable ON amenitiestable.AmenitiesID = roomamenitiestable.AmenitiesID
WHERE
    amenitiestable.Amenities = 'Jacuzzi';
/*
