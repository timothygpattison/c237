SELECT 
    guesttable.FirstName,
    guesttable.LastName,
    reservationtable.RoomNumber,
    reservationtable.StartDate,
    reservationtable.Adults + reservationtable.Children Guests
FROM
    reservationtable
        INNER JOIN
    guesttable ON reservationtable.GuestID = guesttable.GuestID
WHERE
    guesttable.GuestID = 5;
/*
Karie	Yang	201	2023-03-06	4
Karie	Yang	203	2023-09-13	4
*/

SELECT 
    roomtable.RoomNumber,
    reservationtable.ReservationID,
    reservationtable.Cost
FROM
    roomtable
        LEFT OUTER JOIN
    reservationtable ON reservationtable.RoomNumber = roomtable.RoomNumber;
/*
